meta {
  name: Confirm a password reset
  type: http
  seq: 6
}

put {
  url: {{baseUrl}}/api/persons/:personId/password/reset/:reviewId/confirm?decision=APPROVED
  body: none
  auth: inherit
}

params:query {
  decision: APPROVED
}

params:path {
  personId: {{personId}}
  reviewId: {{reviewId}}
}

headers {
  x-device-id: {{deviceId}}
}
