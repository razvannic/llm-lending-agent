meta {
  name: Save an external decision
  type: http
  seq: 1
}

post {
  url: {{baseUrl}}/api/lending-applications/:lendingApplicationId/external-decisions
  body: json
  auth: none
}

params:path {
  lendingApplicationId: {{lendingApplicationId}}
}

body:json {
  {
    "externalDecisionRequestId": "{{externalDecisionRequestId}}",
    "decision": "ACCEPT"
  }
}
